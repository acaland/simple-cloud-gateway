{
  "name": "Job",
  "plural": "jobs",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "jobId": {
      "type": "number",
      "id": true
    },
    "appId": {
      "type": "string",
      "required": true
    },
    "submissionDate": {
      "type": "date",
      "required": true
    },
    "inputZipURL": {
      "type": "string"
    },
    "status": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "prototype.output": {
      "accepts": [

      ],
      "returns": [
        {
          "arg": "data",
          "type": "file",
          "root": true,
          "description": "A zip file containing the output"
        },
        {"arg": "Content-Type", "type": "string", "http": { "target": "header" }},
        {"arg": "Content-Disposition", "type": "string", "http": { "target": "header" }}
      ],
      "description": "Return the output of the current job",
      "http": [
        {
          "path": "/output",
          "verb": "get",
          "errorStatus": 404
        },
        {
          "path": "/getOutput",
          "verb": "get",
          "errorStatus": 404
        }
      ]
    }
  }
}
